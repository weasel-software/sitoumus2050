    "@ngx-translate/http-loader": "^4.0.0",
    "@types/jasmine": "^3.3.0",
    "@types/node": "^10.12.10",
    "async": "^2.5.0",
    "body-parser": "^1.17.2",
    "chalk": "^2.4.1",
    "codelyzer": "^4.5.0",
    "compression": "^1.7.3",
    "concat": "^1.0.3",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
