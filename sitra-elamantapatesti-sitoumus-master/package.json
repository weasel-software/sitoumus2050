  "scripts": {
    "heroku-postbuild": "npm run build",
    "start": "node dist/server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "ng build --prod --output-hashing=none && node build.js",
    "build:client-test": "ng build --prod --configuration test --output-hashing=none && node build.js",
    "build:client-prod": "ng build --prod --configuration production --output-hashing=none && node build.js",
    "build:server": "webpack --config webpack.server.config.js --progress",
    "dev": "npm run dev:client & npm run dev:server",
    "dev:client": "ng serve --port 5500 -o --hmr --proxy-config proxy.conf.json",
    "dev:server": "nodemon --exec ts-node src/server/app.ts --watch src/server/",
    "sync:db": "ts-node src/server/helpers/database-init.ts",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^7.1.1",
